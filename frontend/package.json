{
  "name": "kserve-models-web-app",
  "version": "0.14.0-rc.0",
  "scripts": {
    "ng": "ng",
    "format": "prettier --write './**/*.{ts,html,css}'",
    "build": "npm run copyLibAssets && ng build --configuration production --deploy-url static/",
    "build:fr": "npm run copyLibAssets && ng build --configuration fr --deploy-url static/",
    "build:watch": "npm run copyLibAssets && ng build --configuration production --watch --deploy-url static/ --output-path ../backend/apps/v1beta1/static/ --output-hashing all",
    "build:watch:fr": "npm run copyLibAssets && ng build --configuration fr --watch --deploy-url static/ --output-path ../backend/apps/v1beta1/static/ --output-hashing all",
    "build:watch:ng": "npm run copyLibAssets && ng serve --proxy-config proxy.conf.json",
    "copyLibAssets": "cp -r ./node_modules/kubeflow/assets/* ./src/assets/",
    "format:check": "prettier --check 'src/**/*.{js,ts,html,scss,css}' || node scripts/check-format-error.js",
    "format:write": "prettier --write 'src/**/*.{js,ts,html,scss,css}'",
    "i18n:extract": "ng extract-i18n --output-path i18n",
    "test": "ng test",
    "test:prod": "ng test --browsers=ChromeHeadless --watch=false",
    "lint-check": "ng lint",
    "lint": "ng lint --fix",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.3.0",
    "@angular/cdk": "~14.2.7",
    "@angular/cdk-experimental": "~14.2.7",
    "@angular/common": "~14.3.0",
    "@angular/compiler": "~14.3.0",
    "@angular/core": "~14.3.0",
    "@angular/forms": "~14.3.0",
    "@angular/localize": "^14.3.0",
    "@angular/material": "~14.2.7",
    "@angular/platform-browser": "~14.3.0",
    "@angular/platform-browser-dynamic": "~14.3.0",
    "@angular/router": "~14.3.0",
    "@fortawesome/angular-fontawesome": "^0.11.1",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@kubernetes/client-node": "^0.17.0",
    "ace-builds": "^1.40.0",
    "date-fns": "^1.29.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "material-icons": "^1.10.8",
    "monaco-editor": "^0.33.0",
    "ngx-ace-wrapper": "^14.0.0",
    "rxjs": "~7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.0",
    "@angular-devkit/core": "^14.2.0",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "~14.2.0",
    "@angular/compiler-cli": "~14.3.0",
    "@angular/language-service": "~14.3.0",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "14.18.63",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "eslint": "^8.20.0",
    "jasmine-core": "~4.2.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "prettier": "^2.8.8",
    "protractor": "~7.0.0",
    "ts-node": "~10.9.1",
    "typescript": "~4.8.0"
  }
}
